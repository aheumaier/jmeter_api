<h1>Jmeter_runs</h1>

<table>
  <tr>
    <th>Project</th>
    <th>Description</th>
    <th>Jmx source</th>
    <th>State</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @jmeter_runs.each do |jmeter_run| %>
  <tr>
    <td><%= jmeter_run.project.name %></td>
    <td><%= jmeter_run.description %></td>
    <td><%= jmeter_run.jmx_source %></td>
    <td><%= jmeter_run.state %></td>
    <td><%= link_to 'Show', "/projects/#{jmeter_run.project_id}/jmeter_runs/#{jmeter_run.id}" %></td>
    <td><%= link_to 'Edit', "/projects/#{jmeter_run.project_id}/jmeter_runs/#{jmeter_run.id}/edit" %></td>
    <td><%= link_to 'Destroy', "/projects/#{jmeter_run.project_id}/jmeter_runs/#{jmeter_run.id}", :method => :delete,
                    :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Jmeter run', "/projects/#{@project.id}/jmeter_runs/new" %>
