<h1>Listing settings</h1>

<table>
  <tr>
    <th>Jmx file</th>
    <th>Jtl file</th>
    <th>Jmeter threads</th>
    <th>Remote server</th>
    <th>Jmeter counter</th>
    <th>Jmeter troughput</th>
    <th>Jmeter period</th>
    <th>Jmeter accesslog</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @settings.each do |setting| %>
  <tr>
    <td><%= setting.jmx_file %></td>
    <td><%= setting.jtl_file %></td>
    <td><%= setting.jmeter_threads %></td>
    <td><%= setting.remote_server %></td>
    <td><%= setting.jmeter_counter %></td>
    <td><%= setting.jmeter_troughput %></td>
    <td><%= setting.jmeter_period %></td>
    <td><%= setting.jmeter_accesslog %></td>
    <td><%= link_to 'Show', setting %></td>
    <td><%= link_to 'Edit', edit_setting_path(setting) %></td>
    <td><%= link_to 'Destroy', setting, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Setting', new_setting_path %>
